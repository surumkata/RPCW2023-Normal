extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Contracts List of #{cs[0].NomeInstituicao} (#{nipc}):
    .w3-container
      table.w3-table-all
        tr
          th Id
          th Data Inicio Contrato 
          th Data Fim Contrato 
          th Areas De Investigação
        each c in cs
          tr
            td
              a(href="/" + c._id)=c._id
            td=c.DataInicioContrato
            td=c.DataFimContrato
            td=c.AreasInvestigacao
    footer.w3-container.w3-blue
      h5 Generated by RPCW-TESTE in #{d} - #[a(href="/") Return]